{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "file-upload": {
      "type": "library",
      "root": "libs/file-upload",
      "entryFile": "index",
      "sourceRoot": "libs/file-upload/src",
      "compilerOptions": {
        "tsConfigPath": "libs/file-upload/tsconfig.lib.json"
      }
    },
    "supabase": {
      "type": "library",
      "root": "libs/supabase",
      "entryFile": "index",
      "sourceRoot": "libs/supabase/src",
      "compilerOptions": {
        "tsConfigPath": "libs/supabase/tsconfig.lib.json"
      }
    },
    "swagger": {
      "type": "library",
      "root": "libs/swagger",
      "entryFile": "index",
      "sourceRoot": "libs/swagger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/swagger/tsconfig.lib.json"
      }
    }
  }
}