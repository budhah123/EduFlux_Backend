{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@app/file-upload(|/.*)$": "<rootDir>/../libs/file-upload/src/$1",
    "^@app/supabase(|/.*)$": "<rootDir>/../libs/supabase/src/$1",
    "^@app/swagger(|/.*)$": "<rootDir>/../libs/swagger/src/$1"
  }
}